          this.id++;\r\n                       localStorage.setItem(this.taskList, this.task)\r\n                        console.log(localStorage.getItem(this.taskList, this.task))\r\n                     },   \r\n                     completeTask: function () {\r\n                         if(this.task.completed==true) {\r\n                             this.task.setAttribute(\"class\", \"completed\")\r\n                         }\r\n                     },\r\n                     deleteTask: function () {\r\n                         this.taskList.splice(this.taskList, 1)\r\n                     }              \r\n        }\r\n        \r\n    }\r\n\r\n\r\n</script>\r\n\r\n<style>\r\n    input {\r\n        width: 300px;\r\n        height: 30px;\r\n        background: whitesmoke;\r\n    }\r\n\r\n    button {\r\n        position: absolute;\r\n        width: 40px;\r\n        height: 35px;\r\n        font-size: 30px;\r\n        background: orange;\r\n        border-style: none;\r\n        cursor: pointer;\r\n        border-radius: 5%;\r\n    }\r\n\r\n    button:hover {\r\n        transform: scale(110%);\r\n    }\r\n\r\n\r\n\r\n    .completed{\r\n        text-decoration: line-through;\r\n    }\r\n</style>"],"sourceRoot":"src/components"}]}